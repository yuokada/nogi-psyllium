{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "array",
	"items": {
		"type": "object",
		"required": ["id", "title", "year", "dates", "member_ids"],
		"additionalProperties": false,
		"properties": {
			"id": {
				"type": "string",
				"pattern": "^ul_"
			},
			"title": {
				"type": "string"
			},
			"year": {
				"type": "integer"
			},
			"dates": {
				"type": "array",
				"minItems": 1,
				"items": {
					"type": "string",
					"format": "date"
				}
			},
			"venue": {
				"type": "string"
			},
			"source_url": {
				"type": "string",
				"format": "uri"
			},
			"member_ids": {
				"type": "array",
				"minItems": 1,
				"items": {
					"type": "string"
				}
			},
			"centers": {
				"type": "array",
				"items": {
					"type": "object",
					"required": ["id"],
					"additionalProperties": false,
					"properties": {
						"id": {
							"type": "string"
						},
						"label": {
							"type": "string"
						}
					}
				}
			},
			"absent": {
				"type": "array",
				"items": {
					"type": "object",
					"required": ["id"],
					"additionalProperties": false,
					"properties": {
						"id": {
							"type": "string"
						},
						"note": {
							"type": "string"
						}
					}
				}
			}
		}
	}
}
